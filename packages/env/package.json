{
  "name": "env",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./*": {
      "types": "./src/*.ts",
      "import": "./build/src/*.js"
    }
  },
  "scripts": {
    "format": "prettier . --check --cache --cache-location=node_modules/.cache/.prettiercache",
    "format:fix": "prettier . --write --cache --cache-location=node_modules/.cache/.prettiercache --log-level=warn",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "type": "module",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.6.2"
  }
}
